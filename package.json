{
  "name": "path-next",
  "version": "1.1.2",
  "description": "provides JavaScript functions for getting the next version of an object after making a change at a given path",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src -d lib",
    "clean": "rm -rf coverage lib",
    "cover": "jest src --coverage",
    "cover-open": "open coverage/lcov-report/index.html",
    "format": "prettier --write src/**/*.js",
    "lint": "eslint --quiet src --ext .js",
    "prepublish": "babel src -d lib",
    "test": "jest src",
    "test-watch": "jest --watch src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mvolkmann/path-next.git"
  },
  "author": "R. Mark Volkmann <r.mark.volkmann@gmail.com> (http://mvolkmann.github.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mvolkmann/path-next/issues"
  },
  "engines": {
    "node": ">= 8.0"
  },
  "homepage": "https://github.com/mvolkmann/path-next#readme",
  "jest": {
    "collectCoverage": true,
    "roots": [
      "src"
    ],
    "verbose": true
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^23.4.0",
    "prettier": "^1.13.7"
  }
}
